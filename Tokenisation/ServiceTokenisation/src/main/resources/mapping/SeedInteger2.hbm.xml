<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.service.tokenisation.model.SeedInteger2" table="SeedInteger2">

		<id name="seed" type="int" column="Seed">
			<generator class="increment" />
		</id>
		<property name="seedValue" type="long">
			<column name="SeedValue" not-null="false" />
		</property>
		<property name="seedUsedFlag" type="boolean">
			<column name="SeedUsedFlag" not-null="false" />
		</property>
		<property name="configurationIdentifier" type="int">
			<column name="Vault" not-null="false" />
		</property>

	</class>
</hibernate-mapping>